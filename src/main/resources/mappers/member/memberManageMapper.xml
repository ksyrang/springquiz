<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.care.quiz.membership.dao.IMemberDAO">
	
	
	
	<select id="selectmemberInfo" resultType="com.care.quiz.membership.dto.MemberDTO" parameterType="String" >
		SELECT * FROM member WHERE id=#{id}
	</select>
	<select id="selectpostInfo" resultType="com.care.quiz.membership.dto.PostDTO" parameterType="String" >
		SELECT * FROM post WHERE id=#{id}
	</select>


</mapper>